<Window x:Class="SaveMe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaveMe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closed="Window_Closed" Loaded="Window_Loaded" WindowStyle="None">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="81*"/>
            <ColumnDefinition Width="719*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="206*"/>
        </Grid.RowDefinitions>
        <Button x:Name="Setting" Content="Setting" Margin="0,0,0,5" Grid.Row="1" BorderBrush="Black" Background="White" Click="Setting_Click" Height="44" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="71"/>
        <Frame x:Name="contentFrame" Grid.Column="1" Content="Frame" Grid.Row="1" HorizontalAlignment="Left" Width="699" Height="392" VerticalAlignment="Center" NavigationUIVisibility="Hidden" Margin="10,0,0,0"/>
    </Grid>
</Window>